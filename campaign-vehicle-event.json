{
    "compression": "SNAPPY",
    "diagnosticsMode": "OFF",
    "spoolingMode": "TO_DISK",
    "collectionScheme": {
      "conditionBasedCollectionScheme": {
        "conditionLanguageVersion": 1,
        "expression": "$variable.`Vehicle.ABS.DemoBrakePedalPressure` > 7000",
        "minimumTriggerIntervalMs": 1000,
        "triggerMode": "ALWAYS"
      }
    },
    "postTriggerCollectionDuration": 1000,
    "signalsToCollect": [
      {
        "name": "Vehicle.ECM.DemoEngineTorque"
      },
      {
        "name": "Vehicle.ABS.DemoBrakePedalPressure"
      },
      {
        "name": "Vehicle.ABS.Speed"
      },
      {
        "name": "Vehicle.ABS.Gear"
      },
      {
        "name": "Vehicle.VehicleInfoMessage.VehicleID"
      },
      {
        "name": "Vehicle.VehicleInfoMessage.FuelType"
      }
    ]
  }
  